{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "root": true,
  "extends": "airbnb",
  "rules": {
    "max-len": ["error", 120, 4],
    "global-require": 0,
    "func-names": 0,
    "new-cap": 0,
    "quote-props": 0,
    "default-case": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-loop-func": 0,
    "camelcase": 0,
    "no-else-return": 0,
    "no-confusing-arrow": 0,
    "no-unused-expressions": 0,
    "prefer-template": 0,
    "no-underscore-dangle": 0
  }
}
