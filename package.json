{
  "name": "katelibby",
  "version": "1.0.7",
  "description": "An IRC and Slack Bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/wh-iterabb-it/katelibby"
  },
  "bugs": {
    "url": "https://github.com/wh-iterabb-it/katelibby/issues"
  },
  "homepage": "https://github.com/wh-iterabb-it/katelibby#readme",
  "license": "GPL-3.0",
  "contributors": [
    "Beau Bouchard <https://github.com/BeauBouchard>",
    "Eric Norris <https://github.com/ericnorris>",
    "Eric Turner <https://github.com/codemuch>",
    "Ash Kamel <https://github.com/ajkamel>"
  ],
  "scripts": {
    "cover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "start": "node ./bin/start",
    "test": "node_modules/.bin/mocha test/. --recursive",
    "lint": "node_modules/.bin/eslint --format codeframe .",
    "lint-fix": "node_modules/.bin/eslint --fix --format codeframe ."
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "max-len": [
        "error",
        120,
        4
      ],
      "arrow-body-style": 0,
      "arrow-parens": 0,
      "class-methods-use-this": 0,
      "consistent-return": 0,
      "default-case": 0,
      "func-names": 0,
      "global-require": 0,
      "import/no-dynamic-require": 0,
      "import/no-mutable-exports": 0,
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "new-cap": 0,
      "no-confusing-arrow": 0,
      "no-else-return": 0,
      "no-loop-func": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-unused-expressions": 0,
      "prefer-template": 0,
      "quote-props": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@slack/client": "4.9.0",
    "commander": "2.19.0",
    "google": "2.1.0",
    "mta-gtfs": "1.1.0",
    "striptags": "3.1.1",
    "superagent": "4.1.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.2.0",
    "eslint": "5.14.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "mocha": "5.2.0",
    "nock": "10.0.6",
    "nyc": "13.3.0",
    "pm2": "3.3.0",
    "sinon": "7.2.4",
    "sinon-chai": "3.3.0",
    "sinon-stub-promise": "4.0.0"
  }
}
