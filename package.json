{
  "name": "katelibby",
  "version": "1.0.2",
  "description": "An IRC and Slack Bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/wh-iterabb-it/katelibby"
  },
  "bugs": {
    "url": "https://github.com/wh-iterabb-it/katelibby/issues"
  },
  "homepage": "https://github.com/wh-iterabb-it/katelibby#readme",
  "license": "GPL-3.0",
  "contributors": [
    "Beau Bouchard <https://github.com/BeauBouchard>",
    "Eric Norris <https://github.com/ericnorris>",
    "Eric Turner <https://github.com/codemuch>",
    "Ash Kamel <https://github.com/ajkamel>"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-dist",
    "build-dist": "node node_modules/.bin/babel -d dist . -s --ignore test,node_modules",
    "clean": "rm -rf dist && mkdir -p dist/bin dist/node_modules && cp bin/start dist/bin/start && cp -r node_modules/* dist/node_modules && cp package.json dist/package.json",
    "cover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "start": "node ./dist/bin/start",
    "start-dev": "node ./bin/start",
    "test": "node_modules/.bin/mocha test/. --require @babel/register --recursive",
    "lint": "node_modules/.bin/eslint --format codeframe .",
    "fix-my-shit": "node_modules/.bin/eslint --fix --format codeframe ."
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "max-len": [
        "error",
        120,
        4
      ],
      "arrow-body-style": 0,
      "arrow-parens": 0,
      "class-methods-use-this": 0,
      "consistent-return": 0,
      "default-case": 0,
      "func-names": 0,
      "global-require": 0,
      "import/no-dynamic-require": 0,
      "import/no-mutable-exports": 0,
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "new-cap": 0,
      "no-confusing-arrow": 0,
      "no-else-return": 0,
      "no-loop-func": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-unused-expressions": 0,
      "prefer-template": 0,
      "quote-props": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@slack/client": "4.3.0",
    "bluebird": "3.5.1",
    "commander": "2.15.0",
    "fs": "0.0.1-security",
    "google": "2.1.0",
    "mta-gtfs": "1.1.0",
    "path": "0.12.7",
    "striptags": "3.1.1",
    "superagent": "3.8.3",
    "winston": "3.0.0-rc6"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.48",
    "@babel/register": "7.0.0-beta.48",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
    "codecov": "3.0.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.1",
    "mocha": "5.2.0",
    "nock": "9.3.0",
    "nyc": "12.0.2",
    "pm2": "2.10.4",
    "sinon": "5.0.10",
    "sinon-chai": "3.1.0",
    "sinon-stub-promise": "4.0.0",
    "tough-cookie": "2.4.2"
  }
}
